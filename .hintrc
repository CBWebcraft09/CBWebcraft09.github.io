{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "highest-available-document-mode": "off",
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-decoration-thickness"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 113",
    "not firefox <= 114",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ]
}